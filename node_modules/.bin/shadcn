#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/usabatch/coding/web/node_modules/.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules/shadcn/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/usabatch/coding/web/node_modules/.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules/shadcn/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/shadcn@3.8.5_@types+node@24.10.15_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
fi
