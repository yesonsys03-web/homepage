#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/usabatch/coding/web/node_modules/.pnpm/@antfu+ni@25.0.0/node_modules/@antfu/ni/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/@antfu+ni@25.0.0/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/usabatch/coding/web/node_modules/.pnpm/@antfu+ni@25.0.0/node_modules/@antfu/ni/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/@antfu+ni@25.0.0/node_modules:/Users/usabatch/coding/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nup.mjs" "$@"
else
  exec node  "$basedir/../../bin/nup.mjs" "$@"
fi
